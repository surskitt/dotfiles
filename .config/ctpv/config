set shell "/usr/bin/bash"

preview vclip .vclip {{
    vclip() {
      cp "$(vclip.sh "${f}" thumb)" "$cache_f"
    }

    convert_and_show_image vclip
}}

preview mp4thumb video/* {{
    mp4thumb() {
        ctpv_get_thumb.sh "${f}" "${cache_f}"
    }

    convert_and_show_image mp4thumb
}}

priority mp4thumb
