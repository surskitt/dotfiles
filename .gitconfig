[user]
	email = shane@isda.best
	name = shanedabes
[alias]
    st = status -b --short
    stu = st -unormal
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    l = log --oneline --decorate -10
    lo = log --oneline --decorate
    lg = log --oneline --decorate --graph --all
    who = shortlog -s --
    bn = rev-parse --abbrev-ref HEAD
    pb = push "$(git bn)"
    pa = push --all
    pt = push --tags
    pta = !git push --all && git push --tags
[pull]
	rebase = false
[diff]
	tool = vimdiff
